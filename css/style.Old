@import url('https://fonts.googleapis.com/css2?family=DM+Sans:opsz,wght@9..40,700&family=Inter:wght@300;400;500;600&display=swap');
@import url("https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css");

:root {
    --color-theme: #191919;
    --color-theme-red: #f94d1c;
    --color-theme-yellow: #f9d783;
    --color-theme-background: #f0efe9;
    --color-theme-background-yellow: #f0d082;
    --color-theme-background-gray: rgba(255, 255, 255, 0.9);

    --max-width: 1680px;
}

html, body {
    font-family: 'DM Sans', sans-serif;
    font-family: 'Inter', sans-serif;
    font-weight: 400;
    font-size: 15 px;
}

a {
    color: var(--color-theme);
    text-decoration: none;
}

ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
  }

.__contact-icon-links {
    display: flex;
    justify-content: left;
    align-items: center;
}
.__contact-icon-links a {
    margin-left: 20px;
    margin-right: 20px;
}

.__social-icon-links{
    display: flex;
    justify-content: center;
    align-items: center;
}

.__social-icon-links a {
    margin-left: 17px;
    margin-right: 17px;
}

.__logo{
    display: flex;
    justify-content: flex-end;
}

.__nav-links{
    
    font-family: 'Inter', sans-serif;
    font-size: 15px;
}
    .__nav-links a {
        color: var(--color-theme);
        margin-right: 15px;
    }
    .__nav-links a:hover {
        color: var(--color-theme);
        text-decoration: underline;
    }
    .__nav-links a:active, .__nav-links .__active {
        color: var(--color-theme);
        text-decoration: underline;
    }


footer {
  background-color: #0E1021;
  color: #fff;
  
}
footer .footer-section {
  position: relative;
  z-index: 0;
  padding: 70px 0;
  border-bottom: 1px solid rgba(135, 136, 144, 0.4);

}
footer .footer-section .container {
  display: grid;
  grid-template-columns: 3fr 2fr 2fr 2fr 2fr;

}
footer .footer-section .container .logo {
  padding-right: 80px;

}
footer .footer-section .container .logo p {
  padding-top: 30px;
}
footer .footer-section .container h5 {
  font-family: "DM Sans", sans-serif;
  font-weight: 700;
  font-size: 22px;

}
footer .footer-section .container ul {
    list-style-type: none;
    padding: 0;
    margin: 0;

}
footer .footer-section .container ul li {
  font-family: "Inter", sans-serif;
  font-weight: 500;
  font-size: 16px;
  margin-top: 20px;

}
footer .footer-section .container ul li a {
  color: #A7A7AE;
  text-decoration: none;
}
footer .footer-section .container ul li a:hover {
  color: #fff;
  text-decoration: underline;
}

footer .copyright-section .container{
    display: flex;
    justify-content: space-between;
}
  
